<% include partials/header %>
<h1><%= place.name%></h1>
<img src = "<%= place.image %>" alt = "An image of a campground">
<p><%= place.desc%></p>
<% place.comments.forEach((comment) => { %>
    <p>
        <strong><%= comment.author%></strong> - <%= comment.text %>
    </p>
<% }) %>
<% include partials/footer %>